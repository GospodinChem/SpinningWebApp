@using Microsoft.AspNetCore.Identity
<!DOCTYPE html>
<html lang="en">
<head>
    <title>DLF - The best online fishing shop</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width">

    <!-- Start Include All CSS -->
    <link rel="stylesheet" href="~/assets/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/assets/css/elegant-icons.css" />
    <link rel="stylesheet" href="~/assets/css/fishto-icons.css" />
    <link rel="stylesheet" href="~/assets/css/animate.css" />
    <link rel="stylesheet" href="~/assets/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/assets/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/assets/css/slick.css">
    <link rel="stylesheet" href="~/assets/css/lightcase.css">
    <link rel="stylesheet" href="~/assets/css/theme.css" />
    <link rel="stylesheet" href="~/assets/css/responsive.css" />
    <link rel="stylesheet" href="~/assets/css/bootstrap.css" />
    <!-- End Include All CSS -->
    <!-- Favicon Icon -->
    <link rel="icon" type="image/png" href="/assets/images/favicon.png">
    <!-- Favicon Icon -->
</head>
<body>
    <!-- Header Start -->
    <header class="header-01 h3 head-sticky">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <nav class="navbar navbar-expand-lg">
                        <!-- logo Start-->
                        <a class="navbar-brand" href="index-2.html">
                            <img src="/assets/images/logo.png" alt="logo">
                        </a>
                        <!-- logo End-->
                        <!-- Moblie Btn Start -->
                        <button class="navbar-toggler" type="button">
                            <i class="nss-bars1"></i>
                        </button>
                        <!-- Moblie Btn End -->
                        <!-- Nav Menu Start -->
                        <div class="collapse navbar-collapse justify-content-start">
                            <ul class="navbar-nav ml-5">
                                <li class="menu-item-has-children">
                                    <a href="javascript:void(0);">Начало</a>
                                </li>
                                <li class="menu-item-has-children">
                                    <a href="javascript:void(0);">Категории</a>
                                    <ul class="sub-menu">
                                        <li><a asp-controller="Shop" asp-action="Index" asp-route-categoryName="Аксесоари">Аксесоари</a></li>
                                        <li><a asp-controller="Shop" asp-action="Index" asp-route-categoryName="Блесни">Блесни</a></li>
                                        <li><a asp-controller="Shop" asp-action="Index" asp-route-categoryName="Влакна">Влакна</a></li>
                                        <li><a asp-controller="Shop" asp-action="Index" asp-route-categoryName="Воблери">Воблери</a></li>
                                        <li><a asp-controller="Shop" asp-action="Index" asp-route-categoryName="Въдици">Въдици</a></li>
                                        <li><a asp-controller="Shop" asp-action="Index" asp-route-categoryName="Джиг Глави">Джиг Глави</a></li>
                                        <li><a asp-controller="Shop" asp-action="Index" asp-route-categoryName="Куки">Куки</a></li>
                                        <li><a asp-controller="Shop" asp-action="Index" asp-route-categoryName="Макари">Макари</a></li>
                                        <li><a asp-controller="Shop" asp-action="Index" asp-route-categoryName="Силикони">Силикони</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
                        <!-- Nav Menu End -->
                        <!-- Btn -->
                        <partial name="_LoginPartial" />
                        <!-- Btn -->
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- Header End -->
    <!-- Popup Search Start -->
    <section class="popup_search_sec">
        <div class="popup_search_overlay"></div>
        <div class="pop_search_background">
            <div class="middle_search">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 text-center">
                            <div class="popup_search_form">
                                <form method="get" action="#">
                                    <input type="search" name="s" id="s" placeholder="Въведете име на продукт">
                                    <button type="submit"><i class="nss-search1"></i></button>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Popup Search End -->

    <div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <!-- Footer Start -->
    <footer class="footer">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-6">
                    <aside class="widget">
                        <div class="about-widget">
                            <a href="index-2.html"><img src="assets/images/logo.png" alt="logo"></a>
                            <p>Lorem ipsum dolor sit amet,<br> consectetur adipiscing elit. Duis at<br> est id leo luctus gravida a in ipsum.</p>
                            <div class="ab-social">
                                <a class="fac" target="_blank" href="https://www.facebook.com/"><i class="nss-facebook-f"></i></a>
                                <a class="twi" target="_blank" href="https://www.twitter.com/"><i class="nss-twitter"></i></a>
                                <a class="you" target="_blank" href="https://www.youtube.com/"><i class="nss-youtube"></i></a>
                                <a class="ins" target="_blank" href="https://www.instagram.com/"><i class="nss-instagram"></i></a>
                            </div>
                        </div>
                    </aside>
                </div>
                <div class="col-lg-2 col-md-6">
                    <aside class="widget">
                        <h3 class="widget-title">Useful Links</h3>
                        <ul>
                            <li><a href="index-4.html">Home</a></li>
                            <li><a href="index-4.html">User</a></li>
                            <li><a href="index-4.html">Our Pricing</a></li>
                            <li><a href="index-4.html">Testimonials</a></li>
                            <li><a href="index-4.html">News</a></li>
                        </ul>
                    </aside>
                </div>
                <div class="col-lg-3 col-md-6">
                    <aside class="widget">
                        <h3 class="widget-title">Why Buy From Us</h3>
                        <ul>
                            <li><a href="index-4.html">Shipping & Delivery</a></li>
                            <li><a href="index-4.html">Secure Payment</a></li>
                            <li><a href="index-4.html">Support</a></li>
                            <li><a href="index-4.html">Stores</a></li>
                            <li><a href="index-4.html">Services</a></li>
                            <li><a href="index-4.html">Returns</a></li>
                        </ul>
                    </aside>
                </div>
                <div class="col-lg-3 col-md-6">
                    <aside class="widget widget_mc4wp_form_widget">
                        <h3 class="widget-title">Subscribe</h3>
                        <form class="mc4wp-form" method="post">
                            <input type="email" name="EMAIL" placeholder="Email" required="">
                            <input type="submit" value="Subscribe">
                        </form>
                    </aside>
                    <p>Get the latest updates via email. Any time you may unsubscribe</p>
                </div>
            </div>
            <!-- Copryrgint Start -->
            <div class="row">
                <div class="col-lg-12">
                    <div class="copyright text-center">
                        <p>© 2021 <a href="https://psdtowpwork.com/">Nsstheme.</a> All rights reserved.</p>
                    </div>
                </div>
            </div>
            <!-- Copryrgint End -->
        </div>
    </footer>
    <!-- Footer End -->
    <!-- Start Include All JS -->
    <script src="/assets/js/jquery.js"></script>
    <script src="/assets/js/bootstrap.min.js"></script>
    <script src="/assets/js/jquery.appear.js"></script>
    <script src="/assets/js/owl.carousel.min.js"></script>
    <script src="/assets/js/slick.js"></script>
    <script src="/assets/js/lightcase.js"></script>
    <script src="/assets/js/jquery-ui.js"></script>
    <script src="/assets/js/theme.js"></script>
    <script src="/assets/js/site.js"></script>
    <!-- End Include All JS -->
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
